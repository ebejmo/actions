{
  "name": "actions",
  "version": "0.2.2",
  "description": "jokes, CSS, Workflow",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test-e2e",
    "test-e2e": "cypress open",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebejmo/actions.git"
  },
  "keywords": [
    "css",
    "workflow"
  ],
  "author": "bejmoemil@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.13.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-jest": "^28.8.0",
    "husky": "^9.1.4",
    "jest": "^29.2.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "give-me-a-joke": "^0.5.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
